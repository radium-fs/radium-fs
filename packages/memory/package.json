  "bugs": {
    "url": "https://github.com/radium-fs/radium-fs/issues"
  },
  "peerDependencies": {
    "@radium-fs/core": "workspace:*"
  },
  "devDependencies": {
    "@radium-fs/core": "workspace:*",
    "@types/picomatch": "^4.0.2",
